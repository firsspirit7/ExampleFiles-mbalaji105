<apex:page standardController="Account" recordSetVar="account">
    <apex:pageBlock title="My Content">
        <apex:pageBlockTable value="{!account}" var="item">
            <apex:column value="{!item.name}"/> 
            <apex:column value="{!item.phone}"/>
        </apex:pageBlockTable> 
    </apex:pageBlock> 
</apex:page>